{
  "build": {
    "core": "16f877",
    "extra_flags": "-DPIC16F877",
    "f_cpu": "20000000",
    "mcu": "16f877",
    "variant": "16f877"
  },
  "debug": {
    "tools": {
      "mplab-ice": {
        "server": {
          "executable": "bin/mplabx-ice",
          "arguments": [
            "--chip",
            "PIC16F877"
          ]
        }
      }
    }
  },
  "frameworks": [
    "pic-xc8"
  ],
  "name": "⚠️ UNOFFICIAL PIC16F877",
  "upload": {
    "maximum_ram_size": 368,
    "maximum_size": 8192,
    "protocol": "pickit3",
    "protocols": [
      "pickit2",
      "pickit3",
      "pickit4",
      "mplab-ice",
      "custom"
    ]
  },
  "url": "https://www.microchip.com/en-us/product/PIC16F877",
  "vendor": "Microchip",
  "disclaimers": {
    "unofficial": "⚠️ UNOFFICIAL board support - NOT officially supported by Microchip",
    "experimental": "Experimental community project - use at your own risk",
    "official_tools": "For official support, use MPLAB X IDE from Microchip"
  },
  "examples": {
    "blink_c": {
      "name": "LED Blink (C)",
      "description": "Basic LED blinking example using C language",
      "path": "examples/pic16f877-blink",
      "language": "c",
      "difficulty": "beginner"
    },
    "blink_asm": {
      "name": "LED Blink (Assembly)",
      "description": "Basic LED blinking example using assembly language",
      "path": "examples/pic16f877-asm-blink", 
      "language": "assembly",
      "difficulty": "intermediate"
    }
  },
  "documentation": {
    "getting_started": "https://github.com/s-celles/PIC_Test_Project/blob/main/platform-pic8bit/README.md",
    "examples": "https://github.com/s-celles/PIC_Test_Project/tree/main/platform-pic8bit/examples",
    "datasheet": "https://ww1.microchip.com/downloads/en/devicedoc/39582b.pdf"
  }
}